<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--    鸿洋水平数字Progressbar-->
    <declare-styleable name="HorizontalProgressBarWithNumber">
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />
        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_offset" format="dimension" />
        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <!--HorizontalStyle-->
    <declare-styleable name="HorizontalProgressView">
        <attr name="track_width" format="dimension" />
        <attr name="start_progress" format="integer" />
        <attr name="end_progress" format="integer" />
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="isTracked" format="boolean" />
        <attr name="trackColor" format="color" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressDuration" format="integer" />
        <attr name="progressTextVisibility" format="boolean" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="animateType">
            <enum name="AccelerateDecelerateInterpolator" value="0" />
            <enum name="LinearInterpolator" value="1" />
            <enum name="AccelerateInterpolator" value="2" />
            <enum name="DecelerateInterpolator" value="3" />
            <enum name="OvershootInterpolator" value="4" />
        </attr>
        <attr name="corner_radius" format="dimension" />
        <attr name="text_padding_bottom" format="dimension" />
        <attr name="textMovedEnable" format="boolean" />
    </declare-styleable>

    <!--CircleStyle-->
    <declare-styleable name="CircleProgressView">
        <attr name="track_width" />
        <attr name="start_progress" />
        <attr name="end_progress" />
        <attr name="start_color" />
        <attr name="end_color" />
        <attr name="isTracked" />
        <attr name="trackColor" />
        <attr name="progressDuration" />
        <attr name="progressTextVisibility" />
        <attr name="progressBriefText" format="string" />
        <attr name="progressTextColor" />
        <attr name="progressBriefTextColor" format="color" />
        <attr name="progressTextSize" />
        <attr name="progressBriefTextSize" format="dimension" />
        <attr name="animateType" />
        <attr name="isFilled" format="boolean" />
        <attr name="circleBroken" format="boolean" />
        <attr name="isGraduated" format="boolean" />
        <attr name="scaleZone_width" format="dimension" />
        <attr name="scaleZone_length" format="dimension" />
        <attr name="scaleZone_corner_radius" format="dimension" />
        <attr name="scaleZone_padding" format="dimension" />
    </declare-styleable>

</resources>